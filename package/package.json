{
	"name": "astro-typed-links",
	"version": "1.1.0",
	"description": "An Astro integration to automatically get typed links to your pages.",
	"author": {
		"email": "contact@florian-lefebvre.dev",
		"name": "Florian Lefebvre",
		"url": "https://florian-lefebvre.dev"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"optimization"
	],
	"homepage": "https://github.com/florian-lefebvre/astro-typed-links",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./link": {
			"types": "./dist/link.d.ts",
			"default": "./dist/link.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup"
	},
	"type": "module",
	"peerDependencies": {
		"astro": "^5.0.0"
	},
	"devDependencies": {
		"tsup": "^8.1.0"
	}
}
