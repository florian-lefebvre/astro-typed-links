{
	"name": "astro-typed-links",
	"version": "1.1.5",
	"description": "An Astro integration to automatically get typed links to your pages.",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./link": {
			"types": "./dist/link.d.ts",
			"default": "./dist/link.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"optimization"
	],
	"author": "florian-lefebvre",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/florian-lefebvre/astro-typed-links.git"
	},
	"bugs": "https://github.com/florian-lefebvre/astro-typed-links/issues",
	"homepage": "https://github.com/florian-lefebvre/astro-typed-links#readme",
	"license": "MIT",
	"peerDependencies": {
		"astro": "^5.0.0"
	},
	"devDependencies": {
		"tsup": "^8.1.0"
	}
}
